{
  "meta": {
    "name": "Fog 1-1",
    "layer": 1,
    "storage": 1000,
    "url": "http://google.com"
  },
  "rules": [
    {
      "data_type": "scalar",
      "name": "gps",
      "modules": [
        {
          "package": "modules.text.gps",
          "name": "validate"
        },
        {
          "package": "modules",
          "name": "storage",
          "configs": {
            "mongo_url": "mongodb://localhost:27017/",
            "db_name": "iot-db",
            "col_name": "gps"
          }
        }
      ]
    },
    {
      "data_type": "stream",
      "name": "traffic_camera",
      "modules": [
        {
          "package": "modules.image",
          "name": "human_detection"
        },
        {
          "package": "modules.stream",
          "name": "mjpeg"
        }
      ]
    }
  ],
  "start_up": [
    {
      "name": "traffic_camera",
      "camera_id": "12345",
      "url": 0
    }
  ]
}